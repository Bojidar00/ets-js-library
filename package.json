{
  "name": "ets-js-library",
  "version": "0.14.0",
  "description": "Library for event ticketing system",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "hardhat test",
    "prepare": "husky install",
    "lint": "eslint --fix \"**/*.js\"",
    "lint:ci": "eslint \"**/*.js\"",
    "prettier": "prettier . --write",
    "prettier:ci": "prettier .",
    "coverage": "jest --coverage",
    "buddy": "buddy -e src/ && buddy -e test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Applied-Blockchain-Lab/ets-js-library.git"
  },
  "author": "Quanterall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Applied-Blockchain-Lab/ets-js-library/issues"
  },
  "homepage": "https://github.com/Applied-Blockchain-Lab/ets-js-library#readme",
  "dependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.21.2",
    "buddy.js": "^0.9.3",
    "ethers": "^5.7.1",
    "hardhat": "^2.12.0-esm.1",
    "http-status-codes": "^2.2.0",
    "nft.storage": "^7.0.0",
    "sinon": "^14.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@babel/preset-env": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@web-std/fetch": "^4.1.0",
    "eslint": "^8.24.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1"
  },
  "imports": {
    "#ipfs.utils": "./src/utils/ipfs.utils.js",
    "#config": "./src/configs/index.config.js",
    "#contract.config/*": "./config/*",
    "#contract": "./src/utils/contract.js"
  },
  "lint-staged": {
    "{src,test,tasks}/**/*.js": [
      "npm run prettier",
      "npm run lint",
      "npm run buddy"
    ]
  }
}
